{
  "manifest_version": 3,
  "name": "Network Traffic Logger",
  "version": "1.0",
  "description": "Logs specific network traffic to the website console",
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}